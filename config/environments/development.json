{
    "environment": "development",
    "orgType": "sandbox",
    "instanceUrl": "https://test.salesforce.com",
    "apiVersion": "59.0",
    "deploymentSettings": {
        "testLevel": "RunLocalTests",
        "checkOnly": false,
        "ignoreWarnings": true,
        "rollbackOnError": true
    },
    "sourceDirectory": "force-app",
    "permissionSets": [
        "Admin_Permission_Set",
        "Developer_Permission_Set"
    ],
    "dataSettings": {
        "loadTestData": true,
        "preserveTestUsers": true
    },
    "scratchOrgSettings": {
        "edition": "Developer",
        "features": [
            "EnableSetPasswordInApi",
            "DebugApex",
            "API"
        ],
        "settings": {
            "lightningExperienceSettings": {
                "enableS1DesktopEnabled": true
            },
            "mobileSettings": {
                "enableS1EncryptedStoragePref2": false
            },
            "securitySettings": {
                "passwordPolicies": {
                    "complexity": "AlphaNumeric",
                    "minLength": 8
                }
            }
        },
        "orgPreferences": {
            "enabled": [
                "S1DesktopEnabled",
                "ChatterEnabled"
            ],
            "disabled": []
        }
    }
}